{
  "name": "greensms",
  "version": "2.0.0",
  "description": "GREENSMS API: SMS, Call, Voice, VK, WhatsApp, Viber, HLR, Pay",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "directories": {
    "example": "examples",
    "lib": "lib"
  },
  "type": "module",
  "files": [
    "dist",
    "lib"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "_watch": "rollup -c -w",
    "_lint": "eslint \"{lib,test}/**/*.{js,ts}\" index.d.ts --fix",
    "_jsdoc": "node ./node_modules/jsdoc/jsdoc.js -c jsdoc.conf.json",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "jest --runInBand --config ./test/jest-e2e.json --setupFiles dotenv/config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greensms-ru/greensms-node.git"
  },
  "keywords": [
    "greensms",
    "sms",
    "rest",
    "api"
  ],
  "author": "GREENSMS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/greensms-ru/greensms-node/issues"
  },
  "homepage": "https://github.com/greensms-ru/greensms-node#readme",
  "dependencies": {
    "axios": "1.5.1",
    "humps": "2.0.1",
    "qs": "6.11.2",
    "yup": "1.3.2"
  },
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@rollup/plugin-eslint": "9.0.5",
    "@types/humps": "^2.0.4",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.6",
    "@types/qs": "^6.9.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "coveralls": "3.1.1",
    "cross-env": "7.0.3",
    "dotenv": "16.3.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-order": "^2.1.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-export-all": "^1.4.1",
    "eslint-plugin-sort-exports": "^0.8.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jsdoc": "4.0.2",
    "jshint": "2.13.6",
    "nyc": "15.1.0",
    "prettier": "^2.8.8",
    "rollup": "4.1.4",
    "rollup-plugin-esbuild": "^6.1.0",
    "rollup-plugin-filesize": "10.0.0",
    "supertest": "6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "settings": {
    "moduleName": "greensms",
    "minify": false
  },
  "optionalDependencies": {
    "fsevents": "2.3.3"
  }
}
